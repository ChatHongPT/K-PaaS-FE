{
  "rewrites": [
    {
      "source": "/api/kakao-address",
      "destination": "/api/kakao-address.js"
    },
    {
      "source": "/job/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/visa/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/kakao-address.js": {
      "maxDuration": 10
    }
  }
}